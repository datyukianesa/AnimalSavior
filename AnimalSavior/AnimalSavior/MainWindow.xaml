<Window x:Class="AnimalSavior.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AnimalSavior"
        mc:Ignorable="d"
        Title="Window1" Height="502" Width="461">
    <Grid Background="#FF4CBDD6">
        <Button Content="Look for Vets" HorizontalAlignment="Left" Height="66" Margin="172,90,0,0" VerticalAlignment="Top" Width="121" Background="#FFFDFDFD" Grid.Row="2"/>
        <Button Content="Community" HorizontalAlignment="Left" Height="66" Margin="32,90,0,0" VerticalAlignment="Top" Width="121" Background="#FFFDFDFD" Click="Button_Click" Grid.Row="2"/>
        <Button Content="Profile" Height="66" Margin="303,90,29,0" VerticalAlignment="Top" Width="121" Background="#FFFDFDFD" Grid.Row="2"/>
        <Button Content="My Pets" HorizontalAlignment="Left" Height="66" Margin="244,182,0,0" VerticalAlignment="Top" Width="121" Background="#FFFDFDFD" Grid.Row="2"/>
        <Button Content="Tips &amp; Trick" HorizontalAlignment="Left" Height="66" Margin="97,182,0,0" VerticalAlignment="Top" Width="121" Background="#FFFDFDFD" Grid.Row="2"/>
        <Image Margin="139,50,126,223" Source="pnda peenge.png" Stretch="Fill" RenderTransformOrigin="0.678,-0.485" Grid.RowSpan="3"/>
        <Label
            x:Name="label_Copy4"
            Width="167"
            Height="40"
            Margin="150,0,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Content="Animal Savior"
            FontFamily="Arial rounded"
            FontSize="24"
            FontWeight="Bold" Grid.Row="2" />
        <Button Content="Logout" HorizontalAlignment="Left" VerticalAlignment="Top" Width="107" Margin="325,21,0,0" Background="#FFE66262" Foreground="White" Height="29" FontFamily="Hobo Std" FontSize="16"/>
        <Label
            x:Name="label"
            Margin="13,17,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Content="Main Menu" FontWeight="Bold" FontSize="18" />
        <Grid.RowDefinitions>
            <RowDefinition Height="63*"/>
            <RowDefinition Height="87*"/>
            <RowDefinition Height="220*"/>
        </Grid.RowDefinitions>
    </Grid>
</Window>
