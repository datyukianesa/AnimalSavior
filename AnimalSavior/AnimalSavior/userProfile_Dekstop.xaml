        <Label
            x:Name="label_Copy5"
            Margin="430,195,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Content="Alamat"
            FontSize="16" FontWeight="Bold" Height="34" Width="66" />
        <Rectangle
            Width="238"
            Height="86"
            Margin="432,229,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Fill="#FFE0E0E0" />
        <TextBlock
            x:Name="textBlock_Copy"
            Width="239"
            Height="69"
            Margin="424,238,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            TextAlignment="Center"
            TextWrapping="Wrap"><Run Text="Jl. Lorem ipsum Dolor Sit Amet " /><LineBreak /><Run Text="Rt 4 Rw3, Free Day Fun, " /><LineBreak /><Run Text="Yogyakarta." /></TextBlock>
