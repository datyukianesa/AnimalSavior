            x:Name="label_Copy3"
            Margin="432,52,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Content="Alamat Email"
            FontSize="16" FontWeight="Bold" Height="33" Width="110" />
        <TextBox
            x:Name="textBox_Copy1"
            Width="238"
            Height="25"
            Margin="432,83,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            IsReadOnly="True"
            Text="author@ygmail.com"
            TextWrapping="Wrap" />
        <Label
            x:Name="label_Copy4"
            Margin="430,127,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
